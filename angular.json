{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "user-registration": {
      "projectType": "library",
      "root": "projects/user-registration",
      "sourceRoot": "projects/user-registration/src",
      "prefix": "ur",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/user-registration/tsconfig.lib.json",
            "project": "projects/user-registration/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/user-registration/tsconfig.lib.prod.json"
            }
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/user-registration/src/test.ts",
            "tsConfig": "projects/user-registration/tsconfig.spec.json",
            "karmaConfig": "projects/user-registration/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/user-registration/tsconfig.lib.json",
              "projects/user-registration/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "user-registration-host": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "sass"
        }
      },
      "root": "projects/user-registration-host",
      "sourceRoot": "projects/user-registration-host/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/user-registration-host",
            "index": "projects/user-registration-host/src/index.html",
            "main": "projects/user-registration-host/src/main.ts",
            "polyfills": "projects/user-registration-host/src/polyfills.ts",
            "tsConfig": "projects/user-registration-host/tsconfig.app.json",
            "aot": true,
            "assets": [
              "projects/user-registration-host/src/favicon.ico",
              "projects/user-registration-host/src/assets"
            ],
            "styles": [
              "projects/user-registration-host/src/styles.sass"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "projects/user-registration-host/src/environments/environment.ts",
                  "with": "projects/user-registration-host/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "user-registration-host:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "user-registration-host:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "user-registration-host:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/user-registration-host/src/test.ts",
            "polyfills": "projects/user-registration-host/src/polyfills.ts",
            "tsConfig": "projects/user-registration-host/tsconfig.spec.json",
            "karmaConfig": "projects/user-registration-host/karma.conf.js",
            "assets": [
              "projects/user-registration-host/src/favicon.ico",
              "projects/user-registration-host/src/assets"
            ],
            "styles": [
              "projects/user-registration-host/src/styles.sass"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/user-registration-host/tsconfig.app.json",
              "projects/user-registration-host/tsconfig.spec.json",
              "projects/user-registration-host/e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "projects/user-registration-host/e2e/protractor.conf.js",
            "devServerTarget": "user-registration-host:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "user-registration-host:serve:production"
            }
          }
        }
      }
    }
  },
  "defaultProject": "user-registration"
}
